SUBROUTINE BOOL esPositivo(INT n) DO
  IF (n > 0) THEN
    RETURN TRUE;
  ELSE
    RETURN FALSE;
  END
END

SUBROUTINE VOID countDown(INT from) DO
  WHILE (from > 0) DO
    PRINT("Contando: " + from);
    from = from - 1;
  END
END

